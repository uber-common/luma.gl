{
  "name": "ocular-gatsby-website",
  "description": "GatsbyJS setup for rendering markdown docs.",
  "version": "0.0.0",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "rm -fr .cache && gatsby develop --port 8001",
    "develop": "gatsby develop --port 8001",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "dependencies": {
    "gatsby": "2.0.77",
    "gatsby-plugin-ocular": "../../ocular/modules/gatsby-plugin-ocular/"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "gh-pages": "^2.0.1",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "write-good": "^0.13.1"
  },
  "keywords": [
    "gatsby"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
